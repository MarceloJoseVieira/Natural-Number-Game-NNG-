Level 1/17
rw le_iff_exists_add,
use 1,
simp,

Level 2/17
use 0,
rw add_zero,
refl,

Level 3/17
intro h,
rw le_iff_exists_add at h ⊢,
cases h with c hc,
use c+1,
rw succ_eq_add_one,
rw hc,
simp,

Level 4/17
cases a with x,
refl,
use x+1,
rw succ_eq_add_one,
rw zero_add,
refl,

Level 5/17
cases hab with k1 h1,
cases hbc with k2 h2,
rw h2,
rw h1,
use k1 + k2,
simp,

Level 6/17
cases hab with k1 h1,
cases hba with k2 h2,
rw h2 at h1,
rw add_assoc at h1,
symmetry at h1,
have h3 := eq_zero_of_add_right_eq_self h1,
have h4 := add_right_eq_zero h3,
rw h4 at h2,
rw h2,
rw add_zero,
refl,

Level 7/17
cases h with k h,
symmetry at h,
have h2 := add_right_eq_zero h,
exact h2,

Level 8/17
cases h with c h,
rw h,
use c,
rw succ_add,
refl,

Level 9/17
revert a,
induction b with k1 n,
intro a,
right,
exact zero_le a,
intro a,
cases a with k2 h2,
left,
exact zero_le (succ k1),
cases n k2,
left,
apply succ_le_succ,
exact h,
right,
apply succ_le_succ,
exact h,

Level 10/17
apply le_succ,
refl,

Level 11/17
intro t,
cases t with n h,
intro t,
rw h,
use n,
simp,

Level 12/17
intro h,
cases h with n h,
rw succ_add at h,
have h2 := succ_inj h,
rw h2,
use n,
refl,

Level 13/17
intro h,
cases h with n h,
rw succ_eq_add_one at h,
rw add_assoc at h,
symmetry at h,
have h2 := eq_zero_of_add_right_eq_self h,
rw add_comm at h2,
rw ← succ_eq_add_one at h2,
have h3 := succ_ne_zero n,
exact h3(h2),

Level 14/17
cases h with n h,
rw h,
use n,
rw add_assoc,
refl,

Level 15/17
intro h1,
cases h1 with h2 h3,
cases h2 with k1 h5,
cases k1 with k2 h6,
exfalso,
rw add_zero at h5,
apply h3,
use 0,
rw add_zero b,
symmetry,
exact h5,
use k2,
rw h5,
rw add_succ,
rw succ_add,
refl,

Level 16/17
intro h,
split,
rw succ_eq_add_one at h,
cases h with n h,
rw h,
use 1 + n,
rw add_assoc,
refl,
intro h2,
cases h with n h,
rw h at h2,
cases h2 with m h2,
rw succ_eq_add_one at h2,
rw add_assoc at h2,
rw add_assoc at h2,
symmetry at h2,
have h3 := eq_zero_of_add_right_eq_self (h2),
rw ← add_assoc at h3,
symmetry at h3, 
rw add_comm at h3,
rw add_comm 1 at h3, 
rw ← add_assoc at h3,
rw ← succ_eq_add_one at h3,
symmetry at h3,
have h4 := succ_ne_zero (m+n),
exact h4(h3),

Level 17/17
split,
apply lt_aux_one,
apply lt_aux_two,
