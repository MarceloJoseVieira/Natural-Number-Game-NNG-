Level 1/8
rw pow_zero,
refl,

Level 2/8
rw pow_succ,
rw mul_zero,
refl,

Level 3/8
rw one_eq_succ_zero,
rw pow_succ,
rw pow_zero,
rw one_mul,
refl,

Level 4/8
induction m with x hx,
rw pow_zero,
refl,
rw pow_succ,
rw mul_one,
rw hx,
refl,

Level 5/8
induction n with y hy,
rw add_zero,
rw pow_zero,
rw mul_one,
refl,
rw add_succ,
rw pow_succ,
rw pow_succ,
simp,
rw hy,
refl,

Level 6/8
induction n with x hx,
repeat {rw pow_zero},
rw mul_one,
refl,
repeat {rw pow_succ},
rw hx,
simp,

Level 7/8
induction m with x hx,
rw zero_mul,
rw pow_zero,
rw one_pow,
refl,
rw pow_succ,
rw mul_pow,
rw succ_mul,
rw pow_add,
rw hx,
refl,

Level 8/8
rw two_eq_succ_one,
rw one_eq_succ_zero,
repeat {rw pow_succ},
repeat {rw pow_zero},
repeat {rw one_mul},
repeat {rw mul_add, rw add_mul,},
repeat {rw succ_mul},
repeat {rw zero_mul},
rw zero_add,
repeat {rw add_mul},
simp,
