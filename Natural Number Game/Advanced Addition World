Level 1/13
exact succ_inj(hs),

Level 2/13
apply succ_inj,
apply succ_inj,
exact h,

Level 3/13
intro h,
rw h,
refl,

Level 4/13
split,
exact succ_inj,
intro h,
rw h,
refl,

Level 5/13
intro h,
induction t with n hn,
repeat {rw add_zero at h},
rw h,
refl,
rw hn,
refl,
repeat {rw add_succ at h},
apply succ_inj,
exact h,

Level 6/13
rw add_comm t,
rw add_comm t,
exact add_right_cancel a t b,

Level 7/13
split,
exact add_right_cancel a t b,
intro h,
rw h,
refl,

Level 8/13
intro h,
induction a with x hx,
rw zero_add at h,
apply h,
rw succ_add at h,
apply hx,
apply succ_inj,
exact h,

Level 9/13
symmetry,
apply zero_ne_succ,

Level 10/13
cases b with d,
refl,
rw add_succ at H,
exfalso,
have h1 := succ_ne_zero (a+d),
have h2 := h1(H),
exact h2,

Level 11/13
rw add_comm,
apply add_left_eq_zero,

Level 12/13
rw one_eq_succ_zero,
rw add_succ,
rw add_zero,
refl,

Level 13/13
cases n with k,
apply zero_ne_succ,
intro h1,
have h2 := succ_inj(h1),
symmetry at h2,
rw succ_eq_add_one at h2,
have h3 := eq_zero_of_add_right_eq_self(h2),
rw one_eq_succ_zero at h3,
symmetry at h3,
have h4 := zero_ne_succ 0,
exact h4(h3),
